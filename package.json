    "vue-router": "^4.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.2",
    "@types/node": "^18.11.5",
    "@vitejs/plugin-vue": "^3.0.1",
    "autoprefixer": "^10.4.2",
    "babel-preset-vue": "^2.0.2",
    "change-case": "^4.1.2",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "pascalcase": "^2.0.0",
    "postcss": "^8.4.6",
    "postcss-functions": "^4.0.2",
    "postcss-hexrgba": "^2.0.1",
    "tailwindcss": "^3.0.18",
